class Car:
    def __init__(self,speed , unit):
        self.speed=speed
        self.unit=unit
    def __str__(self):
        label="Car with the maximum speed of {} {} "
        return label.format(self.speed, self.unit)    

class Boat:
    def __init__(self,speed):
        self.speed=speed
    def __str__(self):
        label="Boat with the maximum speed of {} knots"
        return label.format(self.speed)
 
